{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "A Project",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "duration": { "type": "string" },
    "contributors": {
      "type": "array",
      "items": { "type": ["string", "null"] }
    },
    "link": { "type": "string" },
    "brief_introduction": { "type": ["string","null"] },
    "introduction": { "type": "string" }
  },
  "required": [
    "name",
    "duration",
    "contributors",
    "link",
    "brief_introduction",
    "introduction"
  ]
}
